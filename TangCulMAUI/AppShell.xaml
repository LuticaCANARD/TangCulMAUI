<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TangCulMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TangCulMAUI"
    Shell.FlyoutBehavior="Flyout"
    Title="TangCulMAUI">
    <FlyoutItem Title="Cats">
        <ShellContent
              Title="Home"
              ContentTemplate="{DataTemplate local:MainPage}"
              Route="MainPage" />
        <ShellContent
    Title="Home2"
    ContentTemplate="{DataTemplate local:PersonList}"
    Route="PersonList" />
    </FlyoutItem>


</Shell>
